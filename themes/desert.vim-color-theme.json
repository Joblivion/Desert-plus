{
    "name": "Desert+",
    "type": "dark",

    "semanticTokenColors": {
        "variable": "#87ceeb",
        "parameter": "#ffdab9",
        "property": "#ffffff",
        "function": "#86d886",
        "method": "#86d886",
        "class": "#f0e68c",
        "interface": "#f0e68c",
        "enum": "#f0e68c",
        "type": "#f0e68c",
        "typeParameter": "#f0e68c",
        "keyword": "#ffa0a0",
        "modifier": "#f0e68c",
        "comment": "#c1dbe6",
        "string": "#ffa0a0",
        "number": "#ffa0a0",
        "regexp": "#ffdab9",
        "operator": "#ffffff",
        "namespace": "#f0e68c",
        "builtinType": "#f0e68c",
        "enumMember": "#ffa0a0",
        "macro": "#ffa0a0",
        "label": "#f0e68c"
    },
    "semanticClass": "Desert+",

    "colors": {
        "editor.background": "#333333",
        "editor.foreground": "#ffffff",
        "editorCursor.foreground": "#ffffff",
        "editorCursor.background": "#708090",
        "editor.selectionBackground": "#4d4d4d",
        "editor.lineHighlightBackground": "#4d4d4d",
        "editorWarning.foreground": "#f0e68c",
        "editorLineNumber.foreground": "#4d4d4d",
        "editorLineNumber.activeForeground": "#ffff50",
        "editorBracketHighlight.foreground1": "#ffffff",
        "editorBracketHighlight.foreground2": "#ffffff",
        "editorBracketHighlight.foreground3": "#ffffff",
        "editorBracketHighlight.foreground4": "#ffffff",
        "editorBracketHighlight.foreground5": "#ffffff",
        "editorBracketHighlight.foreground6": "#ffffff",
        "editorGroupHeader.tabsBackground":"#4d4d4d",
        "statusBar.background": "#c2bfa5",
        "statusBar.foreground": "#000000",
        "statusBar.noFolderBackground": "#c2bfa5",
        "statusBar.noFolderForeground": "#000000",
        "statusBarItem.hoverBackground": "#d4d0c8",
        "sideBar.background": "#333333",
        "sideBar.foreground": "#ffffff",
        "list.activeSelectionBackground": "#4d4d4d",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "#4d4d4d",
        "list.inactiveSelectionForeground": "#ffffff",
        "list.hoverBackground": "#4d4d4d",
        "list.hoverForeground": "#ffffff",
        "input.background": "#4d4d4d",
        "input.foreground": "#ffffff",
        "input.placeholderForeground": "#a0a0a0",
        "inputOption.activeBorder": "#f0e68c",
        "button.background": "#4d4d4d",
        "button.foreground": "#ffffff",
        "button.hoverBackground": "#c2bfa5",
        "dropdown.background": "#4d4d4d",
        "dropdown.foreground": "#ffffff",
        "panel.background": "#333333",
        "panel.border": "#333333",
        "panelTitle.activeForeground": "#ffffff",
        "panelTitle.inactiveForeground": "#a0a0a0",
        "badge.background": "#4d4d4d",
        "badge.foreground": "#ffffff",
        "tab.border": "#333333",
        "tab.activeForeground":"#ffffff",
        "tab.inactiveForeground": "default",
        "terminal.background": "#333333",
        "terminal.border": "#000000",
        "terminal.foreground": "#ffffff",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiRed": "#ffa0a0",
        "terminal.ansiGreen": "#86d886",
        "terminal.ansiYellow": "#f0e68c",
        "terminal.ansiBlue": "#87ceeb",
        "terminal.ansiMagenta": "#ffa0a0",
        "terminal.ansiCyan": "#ffdab9",
        "terminal.ansiWhite": "#ffffff",
        "titleBar.activeBackground": "#333333",
        "titleBar.inactiveBackground": "#333333",
        "editorOverviewRuler.border": "#00000000"
    },
    "tokenColors": [
        {
            "name": "Comments",
            "scope": [
                "comment",
                "comment.block.documentation",
				"punctuation.definition.comment",
                "comment.block.documentation punctuation",
                "string.quoted.docstring.multi",
                "string.quoted.docstring.multi.python punctuation.definition.string.begin",
                "string.quoted.docstring.multi.python punctuation.definition.string.end",
                "string.quoted.docstring.multi.python constant.character.escape"
            ],
            "settings": {
                "fontStyle": "italic",
                "foreground": "#c1dbe6"
            } 
        },
        {
            "name": "Static, const",
            "scope": [
                "storage.modifier"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "name": "Number, Boolean, Undefined, Null",
            "scope": [
                "variable.other.constant",
                "punctuation.definition.constant",
                "constant.language",
                "constant.numeric",
                "support.constant",
                "constant.other.caps" // Python global
            ],
            "settings": {
                "foreground": "#ffa0a0"
            }
        },
        {
            "name": "String, Symbols",
            "scope": [
                "string",
                "constant.other.symbol",
                "constant.other.key",
                "meta.attribute-selector",
                "string constant.character"
            ],
            "settings": {
                "fontStyle": "",
                "foreground": "#ffa0a0"
            }
        },
        {
            "name": "Colors",
            "scope": [
                "constant.other.color",
                "constant.other.color.rgb-value.hex punctuation.definition.constant"
            ],
            "settings": {
                "foreground": "#ffa0a0"
            }
        },
        {
            "name": "Keyword",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.other.important",
            ],
            "settings": {
                "foreground": "#ffdab9"
            }
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
                "support.variable",
                "string constant.other.placeholder",
                "variable.parameter.handlebars",
                "variable.other.object",
                "meta.fstring",
                "meta.function-call meta.function-call.arguments",
                "meta.embedded.inline.phpx constant.other.php"
            ],
            "settings": {
                "foreground": "#87ceeb"
            }
        },
        {
            "name": "Variable Definition",
            "scope": [
                "meta.definition.variable variable.other.constant",
                "meta.definition.variable variable.other.readwrite",
                "variable.declaration.hcl variable.other.readwrite.hcl",
                "meta.mapping.key.hcl variable.other.readwrite.hcl",
                "variable.other.declaration"
            ],
            "settings": {
                "foreground": "#87ceeb"
            }
        },
        {
            "name": "Function Definition",
            "scope": [
                "entity.name.function",
                "variable.other.enummember",
                "meta.function-call",
                "meta.function-call entity.name.function",
                "variable.function",
                "meta.definition.method entity.name.function",
                "meta.object-literal entity.name.function"
            ],
            "settings": {
                "foreground": "#86d886"
            }
        },
        {
            "name": "Function Argument",
            "scope": [
                "variable.parameter.function.language.special",
                "variable.parameter",
                "meta.function.parameters punctuation.definition.variable",
                "meta.function.parameter variable"
            ],
            "settings": {
                "foreground": "#ffdab9"
            }
        },
        {
            "name": "Class Name",
            "scope": [
                "entity.name",
                "storage.type.built-in",
                "support.type.posix-reserved"
            ],
            "settings": {
                "foreground": "#f0e68c"
            }
        },
    ]
}